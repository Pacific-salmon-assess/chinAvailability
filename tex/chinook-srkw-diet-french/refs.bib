@article{manishinCanLateStage2021,
  title = {Can Late Stage Marine Mortality Explain Observed Shifts in Age Structure of {{Chinook}} Salmon?},
  author = {Manishin, Kaitlyn A. and Cunningham, Curry J. and Westley, Peter A. H. and Seitz, Andrew C.},
  editor = {Fujiwara, Masami},
