@article{fordLinkingKillerWhale2010,
  title = {Linking Killer Whale Survival and Prey Abundance: {{Food}} Limitation in the Oceans' Apex Predator?},
  author = {Ford, John K.B. and Ellis, Graeme M. and Olesiuk, Peter F. and Balcomb, Kenneth C.},
