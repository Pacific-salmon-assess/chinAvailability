@article{stewartSurvivalFattestLinking2021,
  title = {Survival of the Fattest: Linking Body Condition to Prey Availability and Survivorship of Killer Whales},
  shorttitle = {Survival of the Fattest},
  author = {Stewart, Joshua D. and Durban, John W. and Fearnbach, Holly and {Barrett-Lennard}, Lance G. and Casler, Paige K. and Ward, Eric J. and Dapp, Derek R.},
  year = {2021},
  journal = {Ecosphere},
  volume = {12},
