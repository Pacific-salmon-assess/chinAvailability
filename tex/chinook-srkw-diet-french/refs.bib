@article{freshwaterChinookSalmonDepth2024,
  title = {Chinook Salmon Depth Distributions on the Continental Shelf Are Shaped by Interactions between Location, Season, and Individual Condition},
  author = {Freshwater, Cameron and Anderson, Sean C. and Huff, David D. and Smith, Joseph M. and Jackson, Doug and Hendriks, Brian and Hinch, Scott G. and Johnston, Stephen and Trites, Andrew W. and King, Jackie},
